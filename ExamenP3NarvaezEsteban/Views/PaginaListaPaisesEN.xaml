<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:ExamenP3NarvaezEsteban.ViewModels"
             x:Class="ExamenP3NarvaezEsteban.Views.PaginaListaPaisesEN">
    <ContentPage.BindingContext>
        <local:PaginaListaPaisesViewModelEN />
    </ContentPage.BindingContext>
    <StackLayout>
        <ListView x:Name="listaPaisesEN" ItemsSource="{Binding Paises}">
            <ListView.ItemTemplate>
                <DataTemplate>
                    <TextCell Text="{Binding .}" />
                </DataTemplate>
            </ListView.ItemTemplate>
        </ListView>
    </StackLayout>
</ContentPage>
